{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test:unit": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:unit:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:e2e": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:e2e:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:api": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:styles": {
      "outputs": []
    },
    "lint:styles:ci": {
      "outputs": []
    },
    "types:check": {
      "outputs": []
    },
    "nyc:report": {
      "outputs": [".nyc_output/**"]
    },
    "happo": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "start": {
      "dependsOn": ["build", "^build"],
      "cache": false
    },
    "test:api:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "db:migrate:dev": {
      "cache": false
    },
    "db:migrate:deploy": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
